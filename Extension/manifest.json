{
  "manifest_version": 2,

  "name": "Malicious Web Detector [Samvid] ",
  "description": "Extension Developed to help recognize sites as phishing or safe websites.",
  "version": "1.4",


  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popupup.html"
  },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "tabs",
    "<all_urls>"
  ]
}
